#!/bin/bash

pkgName="firefox"
styledName="Firefox"
pkgVersion="127.0"
arch="x86_64"
electronArch="amd64"
website="https://mozilla.org"

debianDeps="firefox"
fedoraDeps="firefox"
archDeps="firefox"

if [[ $1 == "install" ]]; then
    echo "Installed"
elif [[ $1 == "remove" ]]; then
    sudo $nativePkgRemove firefox > /dev/null
fi